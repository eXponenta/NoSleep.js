{
  "name": "mosleep",
  "version": "0.7.0",
  "author": "Muhammad Abdul-Rahim",
  "license": "MIT",
  "description": "Prevent display sleep and enable wake lock in any Android or iOS web browser",
  "keywords": [
    "nosleep",
    "wake lock",
    "wakelock",
    "screen",
    "display",
    "sleep"
  ],
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinyshoe/NoSleep.js.git"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^1.1.3",
    "snazzy": "^8.0.0",
    "standard": "^10.0.2",
    "standard-loader": "^6.0.1",
    "webpack": "^3.0.0"
  },
  "homepage": "https://github.com/shinyshoe/NoSleep.js",
  "bugs": {
    "url": "https://github.com/shinyshoe/NoSleep.js/issues"
  },
  "scripts": {
    "test": "cd src && standard --verbose | snazzy",
    "test:fix": "cd src && standard --fix | snazzy",
    "build": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
